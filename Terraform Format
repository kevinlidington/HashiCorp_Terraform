  ami = ""
  instance_type ="t2.micro"
}

#Input terraform fmt into CLI to reformat commands follwed by the name of your file - "cat main.tf".

provider "aws" {
  region     = "us-east-2"
  access_key = ""
  secret_key = ""
}

resource "aws_instance" "ec2lidington" {
  ami           = ""
  instance_type = "t2.micro"
}
